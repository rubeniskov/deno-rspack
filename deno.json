{
  "tasks": {
    "dev": "deno -A ./build.ts dev",
    "build": "deno -A ./build.ts build",
    "preview": "deno -A ./build.ts preview"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "jsxImportSourceTypes": "@types/react",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "unstable": [
    "sloppy-imports"
  ],
  "imports": {
    "@emotion/react": "npm:@emotion/react@^11.14.0",
    "@emotion/styled": "npm:@emotion/styled@^11.14.0",
    "@mui/icons-material": "npm:@mui/icons-material@^7.0.0-rc.0",
    "@mui/material": "npm:@mui/material@^7.0.0-rc.0",
    "@mui/x-charts": "npm:@mui/x-charts@^7.28.0",
    "@mui/x-data-grid": "npm:@mui/x-data-grid@^7.28.1",
    "@mui/x-data-grid-pro": "npm:@mui/x-data-grid-pro@^7.28.1",
    "@mui/x-date-pickers": "npm:@mui/x-date-pickers@^7.28.0",
    "@mui/x-date-pickers-pro": "npm:@mui/x-date-pickers-pro@^7.28.0",
    "@mui/x-tree-view": "npm:@mui/x-tree-view@^7.28.1",
    "@react-spring/web": "npm:@react-spring/web@^9.7.5",
    "@swc/plugin-emotion": "npm:@swc/plugin-emotion@^9.0.0",
    "@types/react": "npm:@types/react@^19.0.10",
    "@types/react-dom": "npm:@types/react-dom@^19.0.4",
    "clsx": "npm:clsx@^2.1.1",
    "dayjs": "npm:dayjs@^1.11.13",
    "react": "npm:react@^19.0.0",
    "react-dom": "npm:react-dom@^19.0.0"
  }
}
